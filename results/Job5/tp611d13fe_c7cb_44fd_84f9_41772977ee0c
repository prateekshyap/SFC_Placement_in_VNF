#!/bin/sh
export PARALLEL_SERVER_DECODE_FUNCTION='parallel.cluster.generic.communicatingDecodeFcn'
export PARALLEL_SERVER_STORAGE_CONSTRUCTOR='makeFileStorageObject'
export PARALLEL_SERVER_JOB_LOCATION='Job5'
export PARALLEL_SERVER_MATLAB_EXE='/apps/MATLAB/R2021a/bin/worker'
export PARALLEL_SERVER_MATLAB_ARGS='-parallel'
export PARALLEL_SERVER_DEBUG='true'
export MLM_WEB_LICENSE='false'
export PARALLEL_SERVER_STORAGE_LOCATION='/scratch/ppriyadarshini/matlab-mtp/ppriyadarshini/R2021a'
export PARALLEL_SERVER_CMR='/apps/MATLAB/R2021a'
export PARALLEL_SERVER_TOTAL_TASKS='12'
export PARALLEL_SERVER_NUM_THREADS='1'
sbatch --job-name=Job5  --output='/scratch/ppriyadarshini/matlab-mtp/ppriyadarshini/R2021a/Job5/Job5.log' --export=PARALLEL_SERVER_DECODE_FUNCTION,PARALLEL_SERVER_STORAGE_CONSTRUCTOR,PARALLEL_SERVER_JOB_LOCATION,PARALLEL_SERVER_MATLAB_EXE,PARALLEL_SERVER_MATLAB_ARGS,PARALLEL_SERVER_DEBUG,MLM_WEB_LICENSE,PARALLEL_SERVER_STORAGE_LOCATION,PARALLEL_SERVER_CMR,PARALLEL_SERVER_TOTAL_TASKS,PARALLEL_SERVER_NUM_THREADS --ntasks=12 --cpus-per-task=1 '/scratch/ppriyadarshini/matlab-mtp/ppriyadarshini/R2021a/Job5/communicatingJobWrapper.sh'
